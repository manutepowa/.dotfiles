{
  "mcpServers": {
    "zai-mcp-server": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@z_ai/mcp-server"
      ],
      "env": {
        "Z_AI_API_KEY": "api_key",
        "Z_AI_MODE": "ZAI"
      }
    },
    "web-search-prime": {
      "type": "http",
      "url": "https://api.z.ai/api/mcp/web_search_prime/mcp",
      "headers": {
        "Authorization": "api_key"
      }
    },
    "web-reader": {
      "type": "http",
      "url": "https://api.z.ai/api/mcp/web_reader/mcp",
      "headers": {
        "Authorization": "api_key"
      }
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@latest"
      ],
      "env": {}
    },
    "perplexity-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "perplexity-mcp",
        "--model",
        "sonar-pro"
      ],
      "env": {}
    },
    "human-mcp": {
      "command": "npx",
      "args": [
        "@goonnguyen/human-mcp"
      ],
      "env": {}
    }
  }
}
