{
  "eslint.format.enable": false,
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "json",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "coc.source.file.priority": 70,
  "coc.source.buffer.priority": 60,
  "suggest.highPrioritySourceLimit": 20,
  "suggest.languageSourcePriority": 95,
  "suggest.lowPrioritySourceLimit": 90,
  "suggest.enablePreselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.detailField": "menu",
  "suggest.noselect": false,
  "json.format.enable": true,
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": " פּ ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  },
  "coc.preferences.messageLevel": "error",
  "diagnostic.checkCurrentLine": true,
  "tailwindCSS.emmetCompletions": true,
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "javascriptreact": "javascriptreact",
    "typescript": "typescriptreact",
    "typescriptreact": "typescriptreact"
  }
  // "tailwindCSS.jsLanguages": [
  //   "javascript",
  //   "javascriptreact",
  //   "typescript",
  //   "typescriptreact"
  // ]
}
